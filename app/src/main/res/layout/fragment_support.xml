<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="20dp">
            <Toolbar
                android:layout_width="wrap_content"
                android:layout_height="56dp"
                android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                android:background="@color/black"
                android:title="@string/support"
                android:titleTextColor="@color/white"
                app:layout_constraintBottom_toTopOf="@+id/RecyclerView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:popupTheme="@style/ThemeOverlay.AppCompat" />

            <ImageView
                android:layout_width="198dp"
                android:layout_height="181dp"
                android:src="@drawable/supportagent48px"
                app:tint="@color/purple" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Ziyang" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="liang.ziy@northeastern.edu" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:text="Ruby-8"
                android:inputType="text" />

            <ImageView
                android:layout_width="198dp"
                android:layout_height="181dp"
                android:src="@drawable/supportagent48px"
                app:tint="@color/pink" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Komal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="upadhyay.ko@northeastern.edu" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:text="komalup1991"
                android:inputType="text" />

            <ImageView
                android:layout_width="197dp"
                android:layout_height="182dp"
                android:src="@drawable/supportagent48px"
                app:tint="@color/orange"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Manushi" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="manushi.f@northeastern.edu" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:text="manushig"
                android:inputType="text" />

            <ImageView
                android:layout_width="194dp"
                android:layout_height="182dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView8"
                android:src="@drawable/supportagent48px"
                app:tint="@color/green" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rakshit" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
